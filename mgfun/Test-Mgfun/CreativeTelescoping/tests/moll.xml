<tests>
  <test name="moll">
    <fun>1/(x^4+2*a*x^2+1)^(m+1)</fun>
    <tex>\int\frac{1}{(x^4+2ax^2+1)^{m+1}}\text{d}x</tex>
    <var_int name="x" />
    <var name="x" type="diff"/>
    <var name="m" type="shift" />
    <set_val name="m" type="int" inter="1..10"/>
    <set_val name="a" type="float" inter="-100..100"/>
    <set_val name="x" type="float" inter="-100..100"/>
  </test>
</tests>
