<tests>

  <test name="bessel_double">
    <fun>exp(-x)*exp(-y)*BesselJ(1,x)*BesselJ(1,y)*BesselJ(2,c*sqrt(x*y))</fun>
    <tex>\int\!\!\!\!\int e^{-x}e^{-y}J_1(x)J_1(y)J_2(c\sqrt{xy})\text{d}x\text{d}y</tex>
    <var_int name="x,y"/>
    <var name="x" type="diff"/>
    <var name="y" type="diff"/>
    <var name="c" type="diff"/>
    <set_val name="x" type="float" inter="0..10"/>
    <set_val name="y" type="float" inter="0..10"/>
    <set_val name="c" type="float" inter="0..10"/>
  </test>

</tests>
