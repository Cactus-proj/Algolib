<tests>
  <test name="chyzak1">
    <fun>(-1)^(n+r+s)*binomial(n,r)*binomial(n,s)*binomial(n+r,r)*binomial(n+s,s)*binomial(2*n-r-s,n)</fun>
    <var_int name="r,s" />
    <var name="n" type="shift"/>
    <var name="s" type="shift"/>
    <var name="r" type="shift" />
    <set_val name="n" type="int" inter="23..23" />
    <set_val name="r" type="int" inter="11..11" />
    <set_val name="s" type="int" inter="17..17" />
  </test>
</tests>
