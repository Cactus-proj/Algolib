<tests>

  <!-- xZeilb_2.test -->

  <test name="binom6">
    <fun>(-1)^k*binomial(n,k)*binomial(2*k,n)</fun>
    <tex>\sum_{k}(-1)^k{n \choose k}{2k \choose n}</tex>
    <var_int name="k" />
    <var name="n" type="shift" />
    <var name="k" type="shift" />
    <set_val name="n" type="int" inter="20..100" />
    <set_val name="k" type="int" inter="2..19" />
  </test>

