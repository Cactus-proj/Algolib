<tests>
  <test name="binom_triple">
    <fun>binomial(n,i)*binomial(n,j)*binomial(n,k)*binomial(n,n-i-j-k)</fun>
    <var_int name="i,j,k" />
    <var name="n" type="shift"/>
    <var name="i" type="shift"/>
    <var name="j" type="shift" />
    <var name="k" type="shift" />
    <set_val name="n" type="int" inter="15..15" />
    <set_val name="i" type="int" inter="1..1" />
    <set_val name="j" type="int" inter="3..3" />
    <set_val name="k" type="int" inter="5..5" />
  </test>

  <test name="binom_triple2">
    <fun>binomial(n-i,i)*binomial(n-j,j)*binomial(n-k,k)</fun>
    <var_int name="i,j,k" />
    <var name="n" type="shift"/>
    <var name="i" type="shift"/>
    <var name="j" type="shift" />
    <var name="k" type="shift" />
    <set_val name="n" type="int" inter="15..15" />
    <set_val name="i" type="int" inter="1..1" />
    <set_val name="j" type="int" inter="3..3" />
    <set_val name="k" type="int" inter="5..5" />
  </test>

</tests>
