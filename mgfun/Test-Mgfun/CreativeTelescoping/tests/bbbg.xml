<!-- 
     Examples from Elliptic integral evaluations of Bessel moments and
     applications
     Bailey, Borwein, Boradhurst, Glasser
  -->


<tests>
  
  <!-- Equ. (14) page 3 -->
  <test name="bbbg1">
    <tex>\int tK_0(at)J_0(wt)dt</tex>
    <fun>t*BesselK(0,a*t)*BesselJ(0,w*t)</fun>
    <var_int name="t"/>
    <var name="t" type="diff" />
    <var name="a" type="diff" />
    <var name="w" type="diff" />
  </test>


  <!-- Equ. (11) p 3, p 20 -->
  <test name="bbbg2">
    <tex>\int t^{2k+1}I_0(t)K_0(t)^4dt</tex>
    <fun>t^(2*k+1)*BesselI(0,t)*BesselK(0,t)^4</fun>
    <var_int name="t" />
    <var name="t" type="diff" />
    <var name="k" type="shift" />
  </test>

</tests>
