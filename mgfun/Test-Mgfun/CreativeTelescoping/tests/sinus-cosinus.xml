<tests>

  <!-- xZeilb_10.test -->
  <test name="cos1">
    <fun>cos(z*t)/sqrt(1-t^2)</fun>
    <var_int name="t"/>
    <var name="t" type="diff"/>
    <var name="z" type="diff"/>
  </test>

  <test name="sin1">
    <fun>sin(z*t)/sqrt(t^2-1)</fun>
    <var_int name="t"/>
    <var name="t" type="diff"/>
    <var name="z" type="diff"/>
  </test>

  <test name="sin_order8">
    <fun>exp(-t*z) * sin(z/(1+z^2))</fun>
    <var_int name="z"/>
    <var name="t" type="diff"/>
    <var name="z" type="diff"/>
  </test>

</tests>
