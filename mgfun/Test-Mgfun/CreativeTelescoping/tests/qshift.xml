<tests>

  <!-- xZeilb_9.test -->
  <test name="qshift1">
    <fun>1/qfactorial(q,n-k)/qfactorial(q,n+k)</fun>
    <tex>\sum_k \frac{1}{[n-k]![n+k]!}</tex>
    <var_int name="k"/>
    <var name="k" type="qshift"/>
    <var name="n" type="qshift"/>
  </test>

  <test name="qshift2">
    <fun>(-1)^k*q^((5*k^2-k)/2)/qfactorial(q,n-k)/qfactorial(q,n+k)</fun>
    <tex>\sum_k \frac{(-1)^kq^{(5k^2-k)/2}}{[n-k]![n+k]!}</tex>
    <var_int name="k"/>
    <var name="k" type="qshift"/>
    <var name="n" type="qshift"/>
  </test>

</tests>
