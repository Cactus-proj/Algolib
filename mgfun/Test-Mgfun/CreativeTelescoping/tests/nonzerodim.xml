<tests>

  <test name="nonzerodim">
    <fun>binomial(m,k)*(n+k)!/z^n/z^k-(-1)^(n+m)*binomial(n,k)*(m+k)!/z^m/z^k</fun>
    <var_int name="k"/>
    <var name="k" type="shift"/>
    <var name="n" type="shift"/>
    <var name="m" type="shift"/>
    <set_val name="n" type="int" inter="20..40"/>
    <set_val name="m" type="int" inter="20..40"/>
    <set_val name="k" type="int" inter="10..19"/>
    <set_option value="target_dimension=1"/>
  </test>

</tests>
