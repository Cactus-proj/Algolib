<tests>
  <test name="binom_quadruple">
    <fun>binomial(n-i,i)*binomial(n-j,j)*binomial(n-k,k)*binomial(n-l,l)</fun>
    <var_int name="i,j,k,l" />
    <var name="n" type="shift"/>
    <var name="i" type="shift"/>
    <var name="j" type="shift" />
    <var name="k" type="shift" />
    <var name="l" type="shift" />
    <set_val name="n" type="int" inter="23..23" />
    <set_val name="i" type="int" inter="1..1" />
    <set_val name="j" type="int" inter="3..3" />
    <set_val name="k" type="int" inter="5..5" />
    <set_val name="l" type="int" inter="7..7" />
  </test>
</tests>
