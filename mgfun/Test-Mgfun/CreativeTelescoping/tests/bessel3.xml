<tests>

  <test name="bessel_big">
    <fun>x*BesselJ(1,a*x)*BesselI(1,a*x)*BesselY(0,x)*BesselK(0,x)</fun>
    <tex>\int x J_1(ax)I_1(ax)Y_0(x)K_0(x)\text{d}x</tex>
    <var_int name="x" />
    <var name="a" type="diff" />
    <var name="x" type="diff" />
  </test>

</tests>
