<tests>

  <!-- Chyzak 2000 -->
  <test name="binom_double1">
    <fun>(-1)^(n+r+s)*binomial(n,r)*binomial(n,s)*binomial(n+r,r)*binomial(n+s,s)*binomial(2*n-r-s,n)</fun>
    <tex>\sum_s(-1)^{n+r+s}{n \choose r}{n\choose s}{n+r\choose r}{n+s\choose s}{2n-r-s\choose n}</tex>
    <var_int name="s"/>
    <var name="r" type="shift"/>
    <var name="s" type="shift"/>
    <var name="n" type="shift"/>
    <set_val name="n" type="int" inter="40..80"/>
    <set_val name="s" type="int" inter="10..20"/>
    <set_val name="r" type="int" inter="10..20"/>
  </test>

</tests>
