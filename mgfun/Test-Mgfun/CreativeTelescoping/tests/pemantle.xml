<!-- 
     Robin Pemantle, Mark Wilson
     Twenty combinatorial examples of asymptotics derived from multivariate
     generating functions
     2008
  -->


<tests>

  <test name="pem_4_1">
    <fun>1/(1-x-y)</fun>
    <tex>\int \frac{1}{1-x-y}\text{d}x</tex>
    <var_int name="x"/>
    <var name="x" type="diff"/>
    <var name="y" type="diff"/>
  </test>

  <test name="pem_4_2">
    <fun>1/(1-x-y-x*y)</fun>
    <tex>\int\frac{1}{1-x-y-xy}\text{d}x</tex>
    <var_int name="x"/>
    <var name="x" type="diff"/>
    <var name="y" type="diff"/>
  </test>

  <test name="pem_4_4a">
    <fun>1/(1-x-x*y*(1-x)^2)</fun>
    <tex>\int\frac{1}{1-x-xy(1-x)^2}\text{d}x</tex>
    <var_int name="x"/>
    <var name="x" type="diff"/>
    <var name="y" type="diff"/>
  </test>

  <test name="pem_4_4b">
    <fun>1/(1-x-x*y*(1-x)^3)</fun>
    <tex>\int\frac{1}{1-x-xy(1-x)^3}\text{d}x</tex>
    <var_int name="x"/>
    <var name="x" type="diff"/>
    <var name="y" type="diff"/>
  </test>

  <test name="pem_4_4c">
    <fun>1/(1-x-x*y*(1-x)^4)</fun>
    <tex>\int\frac{1}{1-x-xy(1-x)^4}\text{d}x</tex>
    <var_int name="x"/>
    <var name="x" type="diff"/>
    <var name="y" type="diff"/>
  </test>

  <test name="pem_4_4d">
    <tex>\int\frac{1}{1-x-xy(1-x)^5}\text{d}x</tex>
    <fun>1/(1-x-x*y*(1-x)^5)</fun>
    <var_int name="x"/>
    <var name="x" type="diff"/>
    <var name="y" type="diff"/>
  </test>

  <test name="pem_4_4e">
    <fun>1/(1-x-x*y*(1-x)^6)</fun>
    <tex>\int\frac{1}{1-x-xy(1-x)^6}\text{d}x</tex>
    <var_int name="x"/>
    <var name="x" type="diff"/>
    <var name="y" type="diff"/>
  </test>

  <test name="pem_4_4f">
    <fun>1/(1-x-x*y*(1-x)^7)</fun>
    <tex>\int\frac{1}{1-x-xy(1-x)^7}\text{d}x</tex>
    <var_int name="x"/>
    <var name="x" type="diff"/>
    <var name="y" type="diff"/>
  </test>

  <test name="pem_4_6">
    <fun>x*y*(1-x)^3/((1-x)^4-x*y*(1-x-x^2+x^3+x^2*y))</fun>
    <tex>\int\frac{xy(1-x)^3}{(1-x)^4-xy(1-x-x^2+x^3+x^2y)}\text{d}x</tex>
    <var_int name="x"/>
    <var name="x" type="diff"/>
    <var name="y" type="diff"/>
  </test>
  
</tests>
