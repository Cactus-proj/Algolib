<tests>
  <!-- Andrews Paule 1993 -->
  <test name="binom_paule">
    <fun>binomial(i+j,j)^2*binomial(4*n-2*i-2*j,2*n-2*j)</fun>
    <tex>\sum_j\left(\sum_i {i+j \choose j}^2{4n-2i-2j \choose 2n-2j}\right)</tex>
    <var_int name="i,j"/>
    <var name="i" type="shift"/>
    <var name="n" type="shift"/>
    <var name="j" type="shift"/>
    <set_val name="n" type="int" inter="50..100"/>
    <set_val name="i" type="int" inter="1..50"/>
    <set_val name="j" type="int" inter="1..50"/>
  </test>
</tests>
