<tests>

  <!-- Relation de contiguite pour la fonction F4 d'Appell -->
  <test name="f4_appell">
    <fun>pochhammer(m+n,a)*pochhammer(m+n,b)/m!/n!/pochhammer(m,c)/pochhammer(n,
      d)*x^m*y^n</fun>
    <tex>\sum_m\frac{(a)_{m+n}(b)_{m+n}}{m!n!(c)_m(d)_n}x^my^n</tex>
    <var_int name="m"/>
    <var name="x" type="diff"/>
    <var name="m" type="shift"/>
  </test>

</tests>
