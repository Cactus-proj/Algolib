<tests>
  <test name="walk1">
    <fun>-(1+y)*(1+x)/x/y/(-x*y+t*x^2*y+t*x*y^2+t*x+t*y)</fun>
    <var_int name="x,y"/>
    <var name="t" type="diff"/>
    <var name="x" type="diff"/>
    <var name="y" type="diff"/>
  </test>

  <test name="walk2xy">
    <fun>(1+x)*(x-y)*(x^2-y)*(x+y)/(x^2*y*(t*x^2+t*y-x*y+t*x^2*y+t*y^2))</fun>
    <var_int name="x,y"/>
    <var name="t" type="diff"/>
    <var name="x" type="diff"/>
    <var name="y" type="diff"/>
  </test>

  <test name="walk2yx">
    <fun>(1+x)*(x-y)*(x^2-y)*(x+y)/(x^2*y*(t*x^2+t*y-x*y+t*x^2*y+t*y^2))</fun>
    <var_int name="y,x"/>
    <var name="t" type="diff"/>
    <var name="x" type="diff"/>
    <var name="y" type="diff"/>
  </test>
</tests>

