<tests>
  <test name="erickson">
    <fun>(s-t)*(-1+s)*(t-x)/s/t/(-s*t+2*s^2*t+2*t^2+2*x*s-3*s*t^2-3*x*t-3*x*s^2+4*x*s*t)</fun>
    <var_int name="s,t"/>
    <var name="x" type="diff"/>
    <var name="s" type="diff"/>
    <var name="t" type="diff"/>
  </test>
  <test name="erickson2">
    <fun>(s-t)*(-1+s)*(t-x)/s/t/(-s*t+2*s^2*t+2*t^2+2*x*s-3*s*t^2-3*x*t-3*x*s^2+4*x*s*t)</fun>
    <var_int name="t,s"/>
    <var name="x" type="diff"/>
    <var name="s" type="diff"/>
    <var name="t" type="diff"/>
  </test>
</tests>
