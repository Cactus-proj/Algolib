with(Mgfun) :
f := (1+exp(x^2+y)+exp(2*x^2+2*y))^2 :
if dfinite_expr_to_sys(f, F(x::diff, y::diff)) =
{diff(F(x,y),x)-2*x*diff(F(x,y),y), 24*diff(F(x,y),y)-50*diff(diff(F(x,y),y),y
)+35*diff(diff(diff(F(x,y),y),y),y)-10*diff(diff(diff(diff(F(x,y),y),y),y),y)+
diff(diff(diff(diff(diff(F(x,y),y),y),y),y),y)}
then okay else % end if ;
