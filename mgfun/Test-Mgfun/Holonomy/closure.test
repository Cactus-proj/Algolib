with(Ore_algebra):
with(Groebner):
with(Holonomy):

p:=1-(x+y):
f:=1/p:
A:=diff_algebra([Dx,x],[Dy,y],polynom={x,y}):
T:=MonomialOrder(A,tdeg(Dx,Dy,x,y)):
G:=Basis(hypergeom_to_dfinite(f,A),T):
HilbertDimension(G,T):
if %=3 then okay else % fi;
A_loc:=skew_algebra(loc_diff=[Dx,x],loc_diff=[Dy,y],
    polynom={x,y},localization=[u,p]):
T_loc:=MonomialOrder(A_loc,lexdeg([Dx,Dy],[x,y,u])):
holon_closure(G,T,T_loc):
if %=[Dx-Dy,x*Dy+y*Dy-Dy+1] then okay else % fi;
